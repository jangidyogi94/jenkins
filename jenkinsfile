pipeline {
      agent any
      stages {
            stage('Init') {
                  steps {
                        echo 'Hi, Welcome to Pipeline - Yogendra'
                        }
            }
            stage('Build') {
                  steps {
                        echo 'Building Sample Maven Project - Yogendra'
                  }
            }
            stage('Test') {
                  steps {
                        echo "Testing being conducted - Yogendra"
                  }
            }
            stage('Deploy Production') {
                  steps {
                        echo "Deploying in Production Area - Yogendra"
                  }
            }
      }
}
